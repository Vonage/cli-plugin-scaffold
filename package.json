{
  "name": "@vonage/cli-plugin-scaffold",
  "version": "0.0.1",
  "author": "Vonage Dev Rel <devrel@vonage.com>",
  "homepage": "https://github.com/vonage/cli-plugin-scaffold",
  "license": "MIT",
  "bugs": "https://github.com/vonage/cli-plugin-scaffold/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@vonage/cli-utils": "^1.0.0",
    "shelljs": "^0.8.4",
    "tslib": "^1.14.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/cli-plugin-scaffold.git"
  },
  "keywords": [
    "oclif-plugin"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "topics": {
      "scaffold": {
        "hidden": true
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.3",
    "@types/node": "^10.17.60",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  }
}
